{
  "ExchangeDescriptors": {
    "ByBit": {
      "Name": "ByBit",     
      "SocketUrl": "wss://stream-testnet.bybit.com/realtime",
      "BaseUrl": "https://api-testnet.bybit.com",
      "Routes": {
        "GET": {
          "Instrument": {
            "Url": "/v2/public/tickers",
            "Parameter": {
              "symbol": {
                "Domain": "symbol",
                "Required": false
              }
            },
            "Result": "Instrument"
          },
          "OrderBook": {
            "Url": "/v2/public/orderBook/L2",
            "Parameter": {
              "symbol": {
                "Domain": "symbol",
                "Required": true
              }
            },
            "Result": "OrderBook"
          },
          "Order": {
            "Url": "/v2/private/order",
            "Parameter": {
              "order_id": {
                "Domain": "OrderId",
                "Required": false
              },
              "order_link_id": {
                "Domain": "OrderLinkId",
                "Required": false
              },
              "symbol": {
                "Domain": "symbol",
                "Required": true
              }
            },
            "Result" : "Order"
          },
          "Position": {
            "Url": "/v2/private/position/list",
            "Result": "Position"

          },
          "Margin": { //balance is in positiondto
            "Url": "/v2/private/position/list",
            "Result": "Margin"
          },
          "Leverage": {
            "Url": "/user/leverage",
              "Result": "Leverage"
          },
          "Wallet": {
            "Fund": {
              "Url": "/open-api/wallet/fund/records"
            },
            "Withdraw": {
              "Url": "/open-api/wallet/withdraw/list"
            },
            "Result": "WalletHistory"
          },
          "Identity": {
            "Url": "/open-api/api-key"
          }
        },
        "POST": {
          "OrderPlace": {
            "Url": "/v2/private/order/create",
            "Parameter": {
              "side": {
                "Domain": "Side",
                "Type": "OrderSide",
                "Required": true
              },
              "symbol": {
                "Domain": "Symbol",
                "Required": true
              },
              "order_type": {
                "Domain": "OrderType",
                "Type": "OrderType",
                "Required": true
              },
              "qty": {
                "Domain": "OrderQty",
                "Required": true
              },
              "price": {
                "Domain": "Price",
                "Required": false
              },
              "time_in_force": {
                "Domain": "TimeInForce",
                "Type": "TimeInForce",                                       
                "Required": true
              },
              "reduce_only": {
                "Domain": "ReduceOnly",                  
                "Required": false
              },
              "order_link_id": {
                "Domain": "OrderLinkId",
                "Required": false
              }
            },
            "Result": "Order"
          }, //paramer: side[Required], symbol[Required], order_type[Required], qty[Required], price[Required with Type limit], time_in_force[Required], reduce_only, order_link_id
          "OrderReplace": {
            "Url": "/open-api/order/replace",
            "Parameter": {
              "side": {
                "Domain": "Side",
                "Type": "OrderSide",
                "Required": true
              },
              "symbol": {
                "Domain": "Symbol",
                "Required": false

              }

                
            },
            "Result": "Order"
          }, //Parameter:
          "OrderCancelAll": {
            "Url": "/v2/private/order/cancelAll",
            "Parameter": {

            },
            "Result": "Order"
          }, //Parameter:  symbol[Required]
          "OrderCancel": {
            "Url": "/v2/private/order/cancel", //paramer: symbol[Required], order_id, order_link_id
            "Parameter": {

            },
            "Result": "Order"
          },
          "Leverage": {
            "Url": "/user/leverage/save",
            "Parameter": {
              "symbol": {
                "Domain": "Symbol",
                "Required": true
              },
              "leverage": {
                "Domain": "Leverage",
                "Required": true
              }
            },
            "Result": "Leverage"
          }
        } //Parameter: symbol[Required], leverage[Required]
      }
    }
  } //EndBybit
