<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <LangVersion>7.2</LangVersion>
    <PackageId>RxSocketWithIoPipeLines</PackageId>
    <Version>1.0.4</Version>
    <Authors>RiCo.Chen</Authors>
    <Company>RiCo.Chen</Company>
    <Description>Mini wrapper Socket with Rx and IO.Pipelines</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/ricoisme/RxSocket</PackageProjectUrl>
